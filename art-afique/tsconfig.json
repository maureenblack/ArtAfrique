{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6",
      "es2015.promise",
      "es2015.collection",
      "es2015.iterable",
      "es2015.symbol.wellknown",
      "es2016.array.include",
      "es2017.object",
      "es2017.string",
      "es2018.promise",
      "es2019.array",
      "es2020.string",
      "es2020.promise",
      "es2021.string",
      "es2021.promise",
      "es2022.error",
      "es2022.object",
      "es2022.string"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    },
    "types": ["node", "jest", "react", "react-dom"]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
